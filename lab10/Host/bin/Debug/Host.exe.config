<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <system.serviceModel>
    <services>
      <service name="lab10.Service1" behaviorConfiguration="mexBehavior">
        <endpoint address="lab10.Service1.svc" binding="basicHttpBinding" contract="lab10.IService1"></endpoint>
        <endpoint address="lab10.Service1.svc" binding="netTcpBinding" contract="lab10.IService1"></endpoint>    
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"></endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8081/"></add>
            <add baseAddress="net.tcp://localhost:45617/"></add>          
          </baseAddresses>
        </host>  
      </service>
    </services>    
  <behaviors>
    <serviceBehaviors>
      <behavior name="mexBehavior">
        <serviceMetadata httpGetEnabled="true"/>        
      </behavior>
    </serviceBehaviors>
  </behaviors>
  </system.serviceModel>
</configuration>